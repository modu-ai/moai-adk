{
  "name": "moai-adk-docs",
  "version": "1.0.0",
  "description": "MoAI-ADK Online Documentation Site built with Nextra",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
