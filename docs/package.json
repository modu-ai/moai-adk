{
  "name": "moai-adk-docs",
  "version": "0.22.5",
  "description": "MoAI-ADK Documentation built with Nextra",
  "private": true,
  "packageManager": "bun@1.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build && node scripts/build-pagefind.js",
    "build:next": "next build",
    "pagefind:build": "node scripts/build-pagefind.js",
    "pagefind:build:ko": "pagefind --site out --output-subdir pagefind/ko --force-language ko",
    "pagefind:build:en": "pagefind --site out --output-subdir pagefind/en --force-language en",
    "pagefind:build:ja": "pagefind --site out --output-subdir pagefind/ja --force-language ja",
    "pagefind:build:zh": "pagefind --site out --output-subdir pagefind/zh --force-language zh",
    "pagefind:serve": "pagefind --site out --serve",
    "pagefind:test": "node scripts/test-pagefind.js",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "validate": "node scripts/validate.js",
    "analyze": "node scripts/analyze.js",
    "ci": "bun run type-check && bun run lint && bun run build:next && bun run pagefind:build && bun run validate",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@next/third-parties": "^16.0.0",
    "next": "16.0.0",
    "nextra": "^4.6.0",
    "nextra-theme-docs": "^4.6.0",
    "pagefind": "^1.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitest/browser": "^4.0.8",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.4.1",
    "typescript": "5.9.3",
    "vitest": "^4.0.8"
  }
}
