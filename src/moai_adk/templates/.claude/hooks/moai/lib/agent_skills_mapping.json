  "prompt_patterns": {
    "spec_creation": {
      "keywords": ["spec", "specification", "requirement", "planning", "/moai:1-plan"],
      "primary_agent": "workflow-spec",
      "secondary_agents": ["workflow-docs"],
      "skills": ["moai-foundation-core", "moai-lang-unified"],
      "context_files": [".moai/templates/spec.md"]
    },
    "implementation": {
      "keywords": ["implement", "development", "code", "write", "/moai:2-run"],
      "primary_agent": "workflow-tdd",
      "secondary_agents": ["core-planner", "core-quality"],
      "skills": ["moai-essentials-unified", "moai-lang-unified"],
      "context_files": []
    },
    "testing": {
      "keywords": ["test", "unit-test", "validation", "pytest"],
      "primary_agent": "workflow-tdd",
      "secondary_agents": ["infra-devops"],
      "skills": ["moai-playwright-webapp-testing", "moai-core-quality"],
      "context_files": ["tests/", "pytest.ini"]
    },
    "documentation": {
      "keywords": ["document", "docs", "readme", "/moai:3-sync"],
      "primary_agent": "workflow-docs",
      "secondary_agents": ["mcp-context7"],
      "skills": ["moai-docs-unified", "moai-mermaid-diagram-expert"],
      "context_files": ["README.md"]
    },
    "git_operations": {
      "keywords": ["git", "commit", "merge", "branch", "pr"],
      "primary_agent": "core-git",
      "secondary_agents": [],
      "skills": ["moai-foundation-core"],
      "context_files": [".gitignore"]
    },
    "frontend": {
      "keywords": ["frontend", "ui", "ux", "react", "vue", "css"],
      "primary_agent": "code-frontend",
      "secondary_agents": ["design-uiux"],
      "skills": ["moai-lang-unified", "moai-component-designer"],
      "context_files": ["src/", "package.json"]
    },
    "backend": {
      "keywords": ["backend", "api", "server", "fastapi", "django"],
      "primary_agent": "code-backend",
      "secondary_agents": ["data-database"],
      "skills": ["moai-lang-unified", "moai-baas-unified"],
      "context_files": ["src/api/", "pyproject.toml"]
    },
    "security": {
      "keywords": ["security", "auth", "encryption", "vulnerability"],
      "primary_agent": "security-expert",
      "secondary_agents": ["core-quality"],
      "skills": ["moai-universal-ultimate"],
      "context_files": ["src/auth/"]
    },
    "performance": {
      "keywords": ["performance", "optimization", "speed", "benchmark"],
      "primary_agent": "infra-devops",
      "secondary_agents": ["mcp-ultrathink"],
      "skills": ["moai-essentials-unified"],
      "context_files": []
    },
    "deployment": {
      "keywords": ["deploy", "docker", "kubernetes", "cicd"],
      "primary_agent": "infra-devops",
      "secondary_agents": [],
      "skills": ["moai-baas-unified"],
      "context_files": ["Dockerfile", ".github/workflows/"]
    },
    "complex_reasoning": {
      "keywords": ["architecture", "design decision", "strategy"],
      "primary_agent": "mcp-ultrathink",
      "secondary_agents": ["workflow-spec"],
      "skills": ["moai-mcp-integration", "moai-core-claude-code"],
      "context_files": [".moai/config/config.json"]
    }
  }
}
