{
  "buildCommand": "cd docs && mkdocs build",
  "outputDirectory": "docs/site",
  "installCommand": "pip install -r docs/requirements.txt",
  "framework": null,
  "functions": {},
  "rewrites": [
    {
      "source": "/docs/:path*",
      "destination": "/docs/:path*"
    },
    {
      "source": "/",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}