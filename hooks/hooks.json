{
  "PostToolUse": [],
  "PreToolUse": [
    {
      "matcher": "Edit|Write|MultiEdit",
      "hooks": [
        {
          "type": "command",
          "command": "node ${CLAUDE_PLUGIN_ROOT}/hooks/scripts/pre-write-guard.cjs"
        },
        {
          "type": "command",
          "command": "node ${CLAUDE_PLUGIN_ROOT}/hooks/scripts/tag-enforcer.cjs"
        }
      ]
    },
    {
      "matcher": "Bash",
      "hooks": [
        {
          "type": "command",
          "command": "node ${CLAUDE_PLUGIN_ROOT}/hooks/scripts/policy-block.cjs"
        }
      ]
    }
  ],
  "SessionStart": [
    {
      "matcher": "*",
      "hooks": [
        {
          "type": "command",
          "command": "node ${CLAUDE_PLUGIN_ROOT}/hooks/scripts/session-notice.cjs"
        }
      ]
    }
  ],
  "UserPromptSubmit": [],
  "SessionEnd": []
}
